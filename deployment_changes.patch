+#!/bin/bash
+
+# LogicAndStories Deployment Script
+# This script handles the deployment of the LogicAndStories application
+
+set -e  # Exit on any error
+
+echo "üöÄ Starting LogicAndStories Deployment"
+echo "======================================"
+
+# Check if Docker is installed
+if ! command -v docker &> /dev/null; then
+    echo "‚ùå Docker is not installed. Please install Docker first."
+    exit 1
+fi
+
+# Check if Docker Compose is installed
+if ! command -v docker-compose &> /dev/null; then
+    echo "‚ùå Docker Compose is not installed. Please install Docker Compose first."
+    exit 1
+fi
+
+# Check if .env file exists
+if [ ! -f .env ]; then
+    echo "‚ùå .env file not found. Please create a .env file with required environment variables."
+    echo "See .env.example for reference."
+    exit 1
+fi
+
+echo "‚úÖ Prerequisites check passed"
+
+# Validate environment variables
+echo "üîç Validating environment configuration..."
+python env_validator.py
+if [ $? -ne 0 ]; then
+    echo "‚ùå Environment validation failed. Please check your .env file."
+    exit 1
+fi
+
+echo "‚úÖ Environment validation passed"
+
+# Run security tests
+echo "üîí Running security tests..."
+python test_admin_security.py > /dev/null 2>&1
+if [ $? -ne 0 ]; then
+    echo "‚ùå Admin security tests failed."
+    exit 1
+fi
+
+python test_admin_py_security.py > /dev/null 2>&1
+if [ $? -ne 0 ]; then
+    echo "‚ùå Admin.py security tests failed."
+    exit 1
+fi
+
+echo "‚úÖ Security tests passed"
+
+# Build CSS assets
+echo "üì¶ Building CSS assets..."
+if [ -f package.json ]; then
+    npm install
+    npm run build:css
+    echo "‚úÖ CSS assets built"
+else
+    echo "‚ö†Ô∏è  No package.json found, skipping CSS build"
+fi
+
+# Build Docker image
+echo "üê≥ Building Docker image..."
+docker build -t logicandstories:latest .
+if [ $? -ne 0 ]; then
+    echo "‚ùå Docker build failed."
+    exit 1
+fi
+
+echo "‚úÖ Docker image built successfully"
+
+# Stop existing containers
+echo "üõë Stopping existing containers..."
+docker-compose down --remove-orphans
+
+# Start new deployment
+echo "üöÄ Starting new deployment..."
+docker-compose up -d
+
+# Wait for services to be ready
+echo "‚è≥ Waiting for services to be ready..."
+sleep 30
+
+# Health check
+echo "üè• Performing health check..."
+health_response=$(curl -s -o /dev/null -w "%{http_code}" http://localhost:5000/health)
+if [ "$health_response" == "200" ]; then
+    echo "‚úÖ Health check passed"
+else
+    echo "‚ùå Health check failed (HTTP $health_response)"
+    echo "üìã Container logs:"
+    docker-compose logs --tail=50 web
+    exit 1
+fi
+
